<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
	<mapper namespace="com.sundostudio.mapper.EquipmentMapper">
	<insert id="upload">
	INSERT INTO equipment(name,price,holdingAmount,howToUse,imagePath) 
	values(#{name},#{price},#{holdingAmount},#{howToUse},#{imagePath})
	</insert>
	
	</mapper>
	